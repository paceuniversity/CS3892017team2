<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.faxianchina.faxian.Frag_Market">


    <ImageView
        android:id="@+id/bow"
        android:layout_width="57dp"
        android:layout_height="64dp"

        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.155"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.324" />

    <ImageView
        android:id="@+id/sword"
        android:layout_width="57dp"
        android:layout_height="64dp"
        android:layout_marginLeft="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.467"

        android:layout_marginStart="51dp" />

    <ImageView
        android:id="@+id/spear"
        android:layout_width="57dp"
        android:layout_height="64dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginLeft="-6dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/sword"
        app:layout_constraintRight_toLeftOf="@+id/editText11"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.686" />

    <ImageView
        android:id="@+id/horse"
        android:layout_width="57dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.753"

        android:layout_marginLeft="51dp"
        android:layout_marginStart="51dp" />

    <EditText
        android:id="@+id/editText7"
        android:layout_width="49dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/bow"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.349"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginRight="0dp" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="49dp"
        android:layout_height="47dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintLeft_toRightOf="@+id/sword"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.614"
        android:layout_marginRight="178dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="49dp"
        android:layout_height="47dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="178dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.132"
        app:layout_constraintLeft_toRightOf="@+id/sword"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.487" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="49dp"
        android:layout_height="47dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.762"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toRightOf="@+id/horse"
        app:layout_constraintRight_toLeftOf="@+id/button12"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.414" />

    <Button
        android:id="@+id/button10"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="Buy"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/button2"
        app:layout_constraintVertical_bias="0.969"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="53dp"
        app:layout_constraintLeft_toRightOf="@+id/editText7"
        android:layout_marginLeft="50dp"
        app:layout_constraintHorizontal_bias="0.979" />

    <Button
        android:id="@+id/button2"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:layout_marginEnd="53dp"
        android:layout_marginRight="53dp"
        android:text="Buy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.479" />

    <Button
        android:id="@+id/button11"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:layout_marginRight="53dp"
        android:text="Buy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.617"
        android:layout_marginEnd="53dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:layout_marginRight="53dp"
        android:text="Buy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.753"
        android:layout_marginEnd="53dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="81dp"
        android:layout_height="27dp"
        android:text="Market"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.168"
        app:layout_constraintBottom_toTopOf="@+id/editText7"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.814" />
</android.support.constraint.ConstraintLayout>
